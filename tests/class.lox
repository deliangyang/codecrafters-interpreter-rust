class Foo {
    var a;

    fun init(a, b) {
        this.a = a;
        this.b = b;
    }

    fun printx() {
        print this.a;
        print this.b;
    }

    fun aIncr(abc) {
        this.a = this.a + abc;
        print this.a;
    }
}

var x = new Foo(3, 4);
print "------------------ class test: x.printx() ----------------------";
x.printx();
print "------------------ class test: x.aIncr(10) ----------------------";
x.aIncr(10);