var hash = {
    "key": "value",
    "key2": 1,
    "key3": {
        "key4": "value4"
    },
    "key4": [1, 2, 3, 4, 5]
};

hash["key5"] = "value5";

println(hash["key5"]);

println(hash["key"]);

println(hash["key2"]);

println(hash["key3"]["key4"]);

println("--------hash array value: hash[key4][0] ---------");

println(hash["key4"][0]);

println("--------hash array value ---------");


println("-----------------");
for (var key in hash) {
    println(key, ":", hash[key]);
    println("--------x---------");
}